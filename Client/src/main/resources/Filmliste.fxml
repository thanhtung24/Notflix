<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.client.controller.FilmlisteController">
   <children>
      <AnchorPane layoutX="-6.0" prefHeight="600.0" prefWidth="900.0">
         <children>
            <TableView fx:id="tableview_filmliste" layoutX="41.0" layoutY="81.0" prefHeight="347.0" prefWidth="262.0">
               <columns>
                  <TableColumn fx:id="column_filmName" prefWidth="199.0" text="Name" />
               </columns></TableView>
            <Label fx:id="label_notflix" layoutX="41.0" layoutY="22.0" prefHeight="52.0" prefWidth="152.0" text="NOTFLIX" textFill="RED">
               <font>
                  <Font size="33.0" />
               </font>
            </Label>
            <Label fx:id="label_filmAnlegen" layoutX="380.0" layoutY="87.0" prefHeight="34.0" prefWidth="140.0" text="Film anlegen">
               <font>
                  <Font size="23.0" />
               </font></Label>
            <Button fx:id="button_filmManuellAnlegen" layoutX="380.0" layoutY="160.0" mnemonicParsing="false" onAction="#switchToFilmManuellAnlegen" text="manuell">
               <font>
                  <Font size="19.0" />
               </font></Button>
            <Button fx:id="button_filmAutoAnlegen" layoutX="380.0" layoutY="246.0" mnemonicParsing="false" onAction="#switchToFilmManuellAutomatischAnlegen" text="auto">
               <font>
                  <Font size="19.0" />
               </font></Button>
            <Button fx:id="button_abmelden" layoutX="774.0" layoutY="546.0" mnemonicParsing="false" onAction="#abmelden" prefHeight="40.0" prefWidth="115.0" text="Abmelden">
               <font>
                  <Font size="19.0" />
               </font></Button>
            <Button fx:id="button_reportanfragen" layoutX="695.0" layoutY="84.0" mnemonicParsing="false" onAction="#switchToReportanfragen" text="Reportanfragen">
               <font>
                  <Font size="19.0" />
               </font></Button>
         </children></AnchorPane>
   </children>
</AnchorPane>
